{
  "aggs": {
    "custId": {
      "terms": {
        "field": "logmap.cust.id",
        "size": 100000,
        "order": {
          "_key": "desc"
        }
      },
      "aggs": {
        "contactType": {
          "terms": {
            "field": "logmap.contactType.keyword",
            "size": 5,
            "order": {
              "_key": "desc"
            }
          },
          "aggs": {
            "eventType": {
              "terms": {
                "field": "logmap.desc.keyword",
                "size": 50,
                "order": {
                  "_key": "desc"
                },
                "missing": "__missing__"
              },
              "aggs": {
                "uCount": {
                  "cardinality": {
                    "field": "logmap.cust.id"
                  }
                },
                "branchName": {
                  "top_hits": {
                    "docvalue_fields": [
                      "logmap.actor.branchName.keyword"
                    ],
                    "_source": "logmap.actor.branchName.keyword",
                    "size": 1,
                    "sort": [
                      {
                        "@timestamp": {
                          "order": "desc"
                        }
                      }
                    ]
                  }
                },
                "area": {
                  "top_hits": {
                    "docvalue_fields": [
                      "logmap.actor.areaId"
                    ],
                    "_source": "logmap.actor.areaId",
                    "size": 1,
                    "sort": [
                      {
                        "@timestamp": {
                          "order": "desc"
                        }
                      }
                    ]
                  }
                },
                "branchUser": {
                  "top_hits": {
                    "docvalue_fields": [
                      "logmap.actor.username.keyword"
                    ],
                    "_source": "logmap.actor.username.keyword",
                    "size": 1,
                    "sort": [
                      {
                        "@timestamp": {
                          "order": "desc"
                        }
                      }
                    ]
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "size": 0,
  "_source": {
    "excludes": []
  },
  "stored_fields": [
    "*"
  ],
  "script_fields": {},
  "docvalue_fields": [
    {
      "field": "@timestamp",
      "format": "date_time"
    }
  ],
  "query": {
    "bool": {
      "must": [
        {
          "match_all": {}
        },
        {
          "range": {
            "@timestamp": {
              "gte": "[[${gte}]]",
              "lte": "[[${lte}]]",
              "format": "epoch_millis"
            }
          }
        },
        {
          "match_phrase": {
            "logmap.type.keyword": {
              "query": "CONTACT_VERF"
            }
          }
        },
        {
          "bool": {
            "should": [
              {
                "match_phrase": {
                  "logmap.desc.keyword": "CONTACT_VERF_INIT:DONE"
                }
              },
              {
                "match_phrase": {
                  "logmap.desc.keyword": "CONTACT_VERF_COMPLETE:DONE"
                }
              }
            ],
            "minimum_should_match": 1
          }
        }
      ],
      "filter": [],
      "should": [],
      "must_not": []
    }
  },
  "pivot" : [
  	{
	 "rows" : ["custId", "contactType"],
	 "cols" : ["eventType"],
	 "vals" : ["any areaId", "any branchName", "any branchUser", "ucount custId AS uCount", "count * AS total"]
  	},
  	{
	 "rows" : ["areaId_CONTACT_VERF_INIT:DONE AS areaId DEFAULT -", "branchName_CONTACT_VERF_INIT:DONE AS branchName DEFAULT -","branchUser_CONTACT_VERF_INIT:DONE AS branchUser DEFAULT SYSTEM"],
	 "cols" : ["contactType"],
	 "vals" : ["sum uCount_CONTACT_VERF_INIT:DONE AS verification", "sum uCount_CONTACT_VERF_COMPLETE:DONE AS Confirmed"]
  	}
  ]
}