{
  "aggs": {
    "tranx": {
      "date_range": {
        "field": "trnx.creationDate",
        "ranges": [
         {
            "key": "last_week",
            "from": "now-1w",
            "to": null
          },
          {
            "key": "last_day",
            "from": "now-1d"
          },
          {
            "key": "last_month",
            "from": "now-1M"
          },
          {
            "key": "last_year",
            "from": "now-1y"
          }
        ]
      },
      "aggs": {
        "channel": {
          "terms": {
            "field": "trnx.channel.keyword",
            "size": 10,
            "order": {
              "_count": "desc"
            }
          }
        }
      }
    }
  },
  "size": 0,
  "_source": {
    "excludes": []
  },
  "stored_fields": [
    "*"
  ],
  "docvalue_fields": [
    {
      "field": "@timestamp",
      "format": "date_time"
    },
    {
      "field": "customer.creationDate",
      "format": "date_time"
    },
    {
      "field": "customer.dateOfBirth",
      "format": "date_time"
    },
    {
      "field": "customer.lastTransactionDate",
      "format": "date_time"
    },
    {
      "field": "customer.lastUpdateDate",
      "format": "date_time"
    },
    {
      "field": "trnx.creationDate",
      "format": "date_time"
    },
    {
      "field": "trnx.lastUpdateDate",
      "format": "date_time"
    },
    {
      "field": "trnx.trnxDate",
      "format": "date_time"
    },
    {
      "field": "trnx.trnxPaidDate",
      "format": "date_time"
    }
  ],
  "query": {
    "bool": {
      "must": [
        {
          "match_all": {}
        },
        {
          "range": {
            "@timestamp": {
              "gte": "[[${gte}]]",
              "lte": "[[${lte}]]",
              "format": "epoch_millis"
            }
          }
        },
        {
          "match_phrase": {
            "_type": {
              "query": "appxn"
            }
          }
        },
        {
          "bool": {
            "should": [
              {
                "match_phrase": {
                  "trnx.trnxStatus.keyword": "PAID"
                }
              },
              {
                "match_phrase": {
                  "trnx.trnxStatus.keyword": "DONE"
                }
              }
            ],
            "minimum_should_match": 1
          }
        }
      ],
      "filter": [],
      "should": [],
      "must_not": []
    }
  }
}