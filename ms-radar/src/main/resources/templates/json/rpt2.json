{
  "aggs": {
    "area": {
      "terms": {
        "field": "branch.areaName.keyword",
        "size": 5,
        "order": {
          "_count": "desc"
        }
      },
      "aggs": {
        "period": {
          "date_range": {
            "field": "trnx.trnxDate",
            "ranges": [
              {
                "key" : "PrevMonth",
                "from": "now-1M/M",
                "to": "now-1M"
              },
              {
               "key" : "ThisMonth",
                "from": "now-0M/M",
                "to": "now-0M"
              }
            ]
          },
          "aggs": {
            "localCommRecv": {
              "sum": {
                "field": "trnx.localCommRecv"
              }
            },
            "commRecv": {
              "sum": {
                "field": "trnx.commRecv"
              }
            },
            "exchangeGain": {
              "sum": {
                "field": "trnx.exchangeGain"
              }
            },
            "coBankCharges": {
              "sum": {
                "field": "trnx.coBankCharges"
              }
            },
            
            "8-bucket": {
              "date_histogram": {
                "field": "trnx.trnxDate",
                "calendar_interval": "1d",
                "time_zone": "Asia/Calcutta",
                "min_doc_count": 1
              }
            }
            
          }
        }
      }
    }
  },
  "size": 0,
  "_source": {
    "excludes": []
  },
  "stored_fields": [
    "*"
  ],
  "script_fields": {},
  "docvalue_fields": [
    "@timestamp",
    "customer.creationDate",
    "customer.creationDateOnline",
    "customer.dateOfBirth",
    "customer.lastTransactionDate",
    "customer.lastUpdateDate",
    "customer.updateDate",
    "trnx.creationDate",
    "trnx.lastUpdateDate",
    "trnx.trnxDate",
    "trnx.trnxPaidDate",
    "xrate.timestamp"
  ],
  "query": {
    "bool": {
      "must": [
        {
          "match_all": {}
        },
        {
          "range": {
            "@timestamp": {
              "gte": 1562999505752,
              "lte": 1570775505753,
              "format": "epoch_millis"
            }
          }
        },
        {
          "bool": {
            "should": [
              {
                "match_phrase": {
                  "trnx.trnxStatus.keyword": "PAID"
                }
              },
              {
                "match_phrase": {
                  "trnx.trnxStatus.keyword": "DONE"
                }
              }
            ],
            "minimum_should_match": 1
          }
        }
      ],
      "filter": [],
      "should": [],
      "must_not": []
    }
  },
  "pivot" : [
  	{
	 "rows" : ["area"],
	 "cols" : ["period"],
	 "vals" : ["localCommRecv","commRecv","exchangeGain","coBankCharges","count * AS Trnx"],
	 "computed" : ["Revenue=localCommRecv + commRecv + exchangeGain - coBankCharges","RPT=Revenue/Trnx"]
  	}
  ]
}