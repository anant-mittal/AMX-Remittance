{
  "aggs": {
    "fp": {
      "terms": {
        "field": "logmap.client.fp.keyword",
        "order": {
          "_count": "desc"
        },
        "size": 20
      },
      "aggs": {
        "browser": {
          "terms": {
            "field": "logmap.agent.browser.keyword",
            "order": {
              "_count": "desc"
            },
            "size": 10
          },
          "aggs": {
            "os": {
              "terms": {
                "field": "logmap.agent.operatingSystem.keyword",
                "order": {
                  "_count": "desc"
                },
                "size": 5
              },
              "aggs": {
                "ip": {
                  "terms": {
                    "field": "logmap.client.ip.keyword",
                    "order": {
                      "_count": "desc"
                    },
                    "size": 50
                  },
                  "aggs": {
                    "country": {
                      "terms": {
                        "field": "geoip.country_code2.keyword",
                        "order": {
                          "_count": "desc"
                        },
                        "missing": "__missing__",
                        "size": 5
                      },
                      "aggs": {
                        "rg": {
                          "terms": {
                            "field": "geoip.region_code.keyword",
                            "order": {
                              "_count": "desc"
                            },
                            "missing": "__missing__",
                            "size": 5
                          },
                          "aggs": {
                            "city": {
                              "terms": {
                                "field": "geoip.city_name.keyword",
                                "order": {
                                  "_count": "desc"
                                },
                                "missing": "__missing__",
                                "size": 20
                              },
                              "aggs": {
                                "app": {
                                  "terms": {
                                    "field": "logmap.client.ct.keyword",
                                    "order": {
                                      "_count": "desc"
                                    },
                                    "size": 15
                                  },
                                  "aggs": {
                                    "span": {
                                      "date_range": {
                                        "field": "@timestamp",
                                        "ranges": [
                                          {
                                            "key" : "lastminute",
                                            "from": "now-15m",
                                            "to": "now"
                                          },
                                          {
                                           "key" : "lastmonth",
                                            "to": "now-15m",
                                            "from": "now-1m"
                                          }
                                        ],
                                        "time_zone": "Asia/Calcutta"
                                      }
                                    }
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "size": 0,
  "_source": {
    "excludes": []
  },
  "stored_fields": [
    "*"
  ],
  "docvalue_fields": [
    {
      "field": "@timestamp",
      "format": "date_time"
    }
  ],
  "query": {
    "bool": {
      "must": [],
      "filter": [
        {
          "match_all": {}
        },
        {
          "match_phrase": {
            "logmap.desc.keyword": {
              "query": "LOGIN:COMPLETED:DONE"
            }
          }
        },
        {
          "match_phrase": {
            "level.keyword": {
              "query": "AUDIT"
            }
          }
        },
        {
          "range": {
            "@timestamp": {
              "format": "epoch_millis",
              "gte": "now-2m",
              "lte": "now"
            }
          }
        }
      ],
      "should": [],
      "must_not": []
    }
  }
}